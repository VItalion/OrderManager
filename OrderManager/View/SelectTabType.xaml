<Window x:Class="OrderManager.View.SelectTabType"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OrderManager.View"
        xmlns:Behaviors="clr-namespace:OrderManager.ViewModel.Behaviors.TypeTab"
        xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        mc:Ignorable="d"
        Title="SelectTabType" Height="130" Width="200">
    <StackPanel>
        <Button Content="Проекты" Margin="5">
            <i:Interaction.Behaviors>
                <Behaviors:ProjectTabBehavior/>
            </i:Interaction.Behaviors>
        </Button>
        <Button Content="Персоны" Margin="5">
            <i:Interaction.Behaviors>
                <Behaviors:PersonTabBehavior/>
            </i:Interaction.Behaviors>
        </Button>
        <Button Content="Отмена" Margin="5">
            <i:Interaction.Behaviors>
                <Behaviors:CancelBehavior/>
            </i:Interaction.Behaviors>
        </Button>
    </StackPanel>
</Window>
